@article{fernandezSocialNetworksSocial1988,
  title = {Social {{Networks}} and {{Social Movements}}: {{Multiorganizational Fields}} and {{Recruitment}} to {{Mississippi Freedom Summer}}},
  shorttitle = {Social {{Networks}} and {{Social Movements}}},
  author = {Fernandez, Roberto M. and McAdam, Doug},
  date = {1988},
  journaltitle = {Sociological Forum},
  volume = {3},
