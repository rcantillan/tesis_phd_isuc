  title = {Diversity, {{Social Capital}}, and {{Cohesion}}},
  author = {Portes, Alejandro and Vickstrom, Erik},
  date = {2011-08-11},
  journaltitle = {Annual Review of Sociology},
  shortjournal = {Annu. Rev. Sociol.},
  volume = {37},
